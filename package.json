  "scripts": {
    "nx": "nx",
    "start": "nx serve",
    "test": "nx test",
    "lint": "nx lint",
    "lint:affected": "nx affected -t lint --base=main --head=HEAD",
    "lint:affected:fix": "nx affected -t lint --fix --base=main --head=HEAD",
    "tsc:affected": "nx affected -t tsc --base=main --head=HEAD --output-style=stream",
    "test:affected": "nx affected -t test --base=main --head=HEAD",
    "format:affected": "nx affected -t format:write --base=main --head=HEAD",
    "format:fix": "nx format:write && nx affected -t lint --fix --base=main --head=HEAD",
    "run-xroad-proxy": "./scripts/run-xroad-proxy.sh",
    "run-pg-proxy": "./scripts/run-pg-proxy.sh",
    "license-audit": "license-checker --failOn 'GPL; AGPL; UNLICENSED' --summary",
    "openapi-generator": "openapi-generator generate -g typescript-fetch --additional-properties=typescriptThreePlus=true,enumPropertyNamingReplaceSpecialCharacters=true,allowUnicodeIdentifiers=true",
    "openapi-generator:test": "openapi-generator generate -g typescript-fetch -i tmp/swagger/clientConfig.json -o tmp/swagger/gen/fetch  --additional-properties=typescriptThreePlus=true,enumPropertyNamingReplaceSpecialCharacters=true,allowUnicodeIdentifiers=true",
    "codegen": "node scripts/codegen.js",
    "compile-strings:is": "formatjs compile ./apps/official-journal-web/src/i18n/strings/is.json --ast --out-file ./apps/official-journal-web/src/i18n/strings/is-compiled.json",
    "compile-strings": "concurrently \"pnpm:compile-strings:*\"",
    "extract-strings": "formatjs extract 'apps/official-journal-web/src/**/*.{ts,tsx}' --ignore='**/*.d.ts' --out-file ./apps/official-journal-web/src/i18n/strings/is.json && yarn compile-strings",
    "build-projects": "nx run-many --target=build --projects=official-journal-web,official-journal-api,official-journal-admin-api,official-journal-application-api",
    "ojoi-init": "nx run-many official-journal-api:dev-init, official-journal-web:codegen",
    "ojoi-web": "nx run-many --target=serve --parallel --projects=official-journal-web,official-journal-admin-api,official-journal-api",
    "lg:init": "nx run legal-gazette-api:dev-init",
    "lg:seed": "nx run legal-gazette-api:seed",
    "lg:migrate": "nx run legal-gazette-api:migrate",
    "lg:update-clients": "nx run-many -t update-client -p legal-gazette-web,legal-gazette-public-web,legal-gazette-application-web --parallel",
    "postinstall": "bash scripts/patch-submodules.sh"
  },
