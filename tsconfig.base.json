{
  "compilerOptions": {
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "module": "esnext",
    "target": "es2015",
    "baseUrl": ".",
    "declaration": false,
    "sourceMap": true,
    "importHelpers": true,
    "allowSyntheticDefaultImports": true,
    "moduleResolution": "node",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "skipDefaultLibCheck": true,
    "types": ["node", "Multer", "../../node_modules/@types/node"],
    "typeRoots": ["node_modules/@types"],
    "paths": {
      "@dmr.is/api-client/*": ["libs/shared/api-client/src/lib/*"],
      "@dmr.is/apm": ["libs/apm/src/index.ts"],
      "@dmr.is/auth/*": ["libs/shared/auth/src/lib/*"],
      "@dmr.is/clients/national-registry": [
        "libs/clients/national-registry/index.ts"
      ],
      "@dmr.is/clients/company-registry": [
        "libs/clients/company-registry/index.ts"
      ],
      "@dmr.is/constants": ["libs/shared/constants/src/index.ts"],
      "@dmr.is/db": ["libs/shared/sequelize/src/index.ts"],
      "@dmr.is/decorators": ["libs/shared/decorators/src/index.ts"],
      "@dmr.is/legal-gazette/dto": ["libs/legal-gazette/dto/src/index.ts"],
      "@dmr.is/legal-gazette/schemas": [
        "libs/legal-gazette/schemas/src/index.ts"
      ],
      "@dmr.is/logging": ["libs/logging/src/index.ts"],
      "@dmr.is/logging-next": ["libs/logging-next/src/index.ts"],
      "@dmr.is/middleware": ["libs/shared/middleware/src/index.ts"],
      "@dmr.is/mocks": ["libs/mocks/src/index.ts"],
      "@dmr.is/modules": ["libs/shared/modules/src/index.ts"],
      "@dmr.is/modules/guards/auth": [
        "libs/shared/modules/src/guards/auth/index.ts"
      ],
      "@dmr.is/nx-open-next": ["libs/nx-open-next/src/index.ts"],
      "@dmr.is/pipelines": ["libs/shared/pipelines/src/index.ts"],
      "@dmr.is/shared/dto": ["libs/shared/dto/src/lib/index.ts"],
      "@dmr.is/shared/filters": ["libs/shared/filters/src/index.ts"],
      "@dmr.is/shared/interceptors": ["libs/shared/interceptors/src/index.ts"],
      "@dmr.is/shared/middleware": ["libs/shared/middleware/src/index.ts"],
      "@dmr.is/shared/models/base": ["libs/shared/models/base/src/index.ts"],
      "@dmr.is/trpc/client/*": ["libs/shared/trpc/src/lib/client/*"],
      "@dmr.is/trpc/utils/*": ["libs/shared/trpc/src/lib/utils/*"],
      "@dmr.is/types": ["libs/shared/types/types/src/index.ts"],
      "@dmr.is/ui/components/*": ["libs/shared/dmr-ui/src/lib/components/*"],
      "@dmr.is/ui/components/island-is": [
        "libs/shared/dmr-ui/src/lib/island-is/index.ts"
      ],
      "@dmr.is/ui/hooks/*": ["libs/shared/dmr-ui/src/lib/hooks/*"],
      "@dmr.is/ui/island-is/theme": [
        "libs/shared/dmr-ui/src/lib/island-is/theme/index.ts"
      ],
      "@dmr.is/ui/lazy/components/*": [
        "libs/shared/dmr-ui-lazy/src/lib/components/*"
      ],
      "@dmr.is/ui/utils/*": ["libs/shared/dmr-ui/src/lib/utils/*"],
      "@dmr.is/utils": ["libs/shared/utils/src/index.ts"],
      "@dmr.is/utils/cache": ["libs/shared/utils/src/lib/cacheUtils.ts"],
      "@dmr.is/utils/cache.provider": [
        "libs/shared/utils/src/lib/cache.provider.ts"
      ],
      "@dmr.is/utils/client": ["libs/shared/utils/src/client.ts"],
      "@island.is/auth-nest-tools": [
        "submodules/island.is/libs/auth-nest-tools/src/lib/auth.ts"
      ],
      "@island.is/island-ui/core": [
        "submodules/island.is/libs/island-ui/core/src/index.ts"
      ],
      "@island.is/island-ui/theme": [
        "submodules/island.is/libs/island-ui/theme/src/index.ts"
      ],
      "@island.is/island-ui/utils": [
        "submodules/island.is/libs/island-ui/utils/src/index.ts"
      ],
      "@island.is/island-ui/vanilla-extract-utils": [
        "submodules/island.is/libs/island-ui/vanilla-extract-utils/src/index.ts"
      ],
      "@island.is/regulations/styling": [
        "submodules/island.is/libs/regulations/src/lib/styling.ts"
      ],
      "@island.is/shared/constants": [
        "submodules/island.is/libs/shared/constants/src/index.ts"
      ],
      "@island.is/shared/types": [
        "submodules/island.is/libs/shared/types/src/index.ts"
      ],
      "@island.is/shared/utils": [
        "submodules/island.is/libs/shared/utils/src/index.ts"
      ]
    }
  },
  "exclude": ["node_modules", "tmp"]
}
