{
  "compilerOptions": {
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "module": "esnext",
    "target": "es2015",
    "baseUrl": ".",
    "declaration": false,
    "sourceMap": true,
    "importHelpers": true,
    "allowSyntheticDefaultImports": true,
    "moduleResolution": "node",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "skipDefaultLibCheck": true,
    "types": [
      "node",
      "multer"
    ],
    "typeRoots": [
      "node_modules/@types"
    ],
    "paths": {
      "@dmr.is/api-client/*": [
        "libs/shared/api-client/src/lib/*"
      ],
      "@dmr.is/apm": [
        "libs/apm/src/index.ts"
      ],
      "@dmr.is/auth/*": [
        "libs/shared/auth/src/lib/*"
      ],
      "@dmr.is/clients/company-registry": [
        "libs/clients/company-registry/index.ts"
      ],
      "@dmr.is/clients/national-registry": [
        "libs/clients/national-registry/src/index.ts"
      ],
      "@dmr.is/constants": [
        "libs/shared/constants/src/index.ts"
      ],
      "@dmr.is/db": [
        "libs/shared/sequelize/src/index.ts"
      ],
      "@dmr.is/decorators": [
        "libs/shared/decorators/src/index.ts"
      ],
      "@dmr.is/legal-gazette/dto": [
        "libs/legal-gazette/dto/src/index.ts"
      ],
      "@dmr.is/legal-gazette/schemas": [
        "libs/legal-gazette/schemas/src/index.ts"
      ],
      "@dmr.is/logging": [
        "libs/logging/src/index.ts"
      ],
      "@dmr.is/logging-next": [
        "libs/logging-next/src/index.ts"
      ],
      "@dmr.is/middleware": [
        "libs/shared/middleware/src/index.ts"
      ],
      "@dmr.is/mocks": [
        "libs/mocks/src/index.ts"
      ],
      "@dmr.is/nx-open-next": [
        "libs/nx-open-next/src/index.ts"
      ],
      "@dmr.is/ojoi/modules": [
        "libs/official-journal/modules/src/index.ts"
      ],
      "@dmr.is/ojoi/modules/guards/auth": [
        "libs/official-journal/modules/src/guards/auth/index.ts"
      ],
      "@dmr.is/pipelines": [
        "libs/shared/pipelines/src/index.ts"
      ],
      "@dmr.is/shared/dto": [
        "libs/shared/dto/src/lib/index.ts"
      ],
      "@dmr.is/shared/filters": [
        "libs/shared/filters/src/index.ts"
      ],
      "@dmr.is/shared/interceptors": ["libs/shared/interceptors/src/index.ts"],
      "@dmr.is/shared/middleware": ["libs/shared/middleware/src/index.ts"],
      "@dmr.is/shared/models/base": ["libs/shared/models/base/src/index.ts"],
      "@dmr.is/trpc/client/*": ["libs/shared/trpc/src/lib/client/*"],
      "@dmr.is/trpc/utils/*": [
        "libs/shared/trpc/src/lib/utils/*"
      ],
      "@dmr.is/types": [
        "libs/shared/types/types/src/index.ts"
      ],
      "@dmr.is/ui/globalStyles": [
        "libs/shared/dmr-ui/src/utils/globalStyles.ts"
      ],
      "@dmr.is/ui/globalCss": [
        "libs/shared/dmr-ui/src/styles/global.css.ts"
      ],
      "@dmr.is/ui/components/*": ["libs/shared/dmr-ui/src/lib/components/*"],
      "@dmr.is/ui/components/island-is": [
        "libs/shared/dmr-ui/src/lib/island-is/index.ts",
      ],
      "@dmr.is/ui/components/island-is/*": [
