            ],
            "example": [
              "NotFound",
              "ValidationError",
              "ForeignKeyConstraintError",
              "UniqueConstraintError",
              "TimeoutError",
              "Unauthorized",
              "Forbidden",
              "BadRequest",
              "InternalServerError",
              "UnknownError"
            ]
          },
          "message": { "type": "string" },
          "details": { "type": "array", "items": { "type": "string" } }
        },
        "required": ["statusCode", "timestamp"]
      },
      "TypeEnum": { "type": "string", "enum": ["Almenn auglýsing"] },
