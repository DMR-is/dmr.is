          "title": {
            "description": "Title of the entity",
            "example": "Base entity",
            "x-enumNames": ["CommonAdvert"],
            "allOf": [{ "$ref": "#/components/schemas/TypeEnum" }]
          },
          "slug": {
            "type": "string",
            "description": "Slug of the entity",
            "example": "base-entity"
          }
        },
        "required": ["id", "title", "slug"]
      },
      "GetTypesDto": {
        "type": "object",
