{
  "extends": ["../../.eslintrc.yml"],
  "ignorePatterns": ["!**/*"],
  "overrides": [
    {
      "files": ["*.ts", "*.tsx", "*.js", "*.jsx"],
      "rules": {
        "typescript-eslint/naming-convention": {
          "selector": "import",
          "format": ["camelCase", "PascalCase"]
        }
      }
    },
    {
      "files": ["*.ts", "*.tsx"],
      "rules": {}
    },
    {
      "files": ["*.js", "*.jsx"],
      "rules": {}
    }
  ]
}
